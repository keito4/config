// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "ctrl+enter",
    "command": "jupyter.execCurrentCell",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+]",
    "command": "workbench.action.terminal.focusNext"
  },
  {
    "key": "ctrl+[",
    "command": "workbench.action.terminal.focusPrevious"
  },
  {
    "key": "cmd+y",
    "command": "redo",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+z",
    "command": "-redo",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+j",
    "command": "-editor.action.joinLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+d",
    "command": "editor.action.openDeclarationToTheSide",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "cmd+k f12",
    "command": "-editor.action.openDeclarationToTheSide",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "cmd+l",
    "command": "editor.action.selectHighlights",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+l",
    "command": "-editor.action.selectHighlights",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+shift+r",
    "command": "-editor.action.formatDocument",
    "when": "editorLangId == 'ruby'"
  },
  {
    "key": "cmd+k cmd+r",
    "command": "editor.action.formatDocument",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+f",
    "command": "-editor.action.formatDocument",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+cmd+[",
    "command": "workbench.action.increaseViewSize"
  },
  {
    "key": "ctrl+cmd+]",
    "command": "workbench.action.decreaseViewSize"
  },
  {
    "key": "ctrl+alt+d",
    "command": "editor.action.goToDeclaration",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "f12",
    "command": "-editor.action.goToDeclaration",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "ctrl+shift+cmd+]",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+shift+cmd+[",
    "command": "workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+cmd+left",
    "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "pageup",
    "command": "-workbench.action.terminal.scrollUpPage",
    "when": "terminalFocus"
  },
  {
    "key": "shift+alt+cmd+pageup",
    "command": "-cursorColumnSelectPageUp",
    "when": "textInputFocus"
  },
  {
    "key": "alt+up alt+",
    "command": "workbench.action.terminal.scrollDown",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+cmd+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "shift+cmd+p",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "cmd+k cmd+r",
    "command": "-workbench.action.keybindingsReference"
  },
  {
    "key": "ctrl+l ctrl+c",
    "command": "workbench.action.toggleEditorGroupLayout"
  },
  {
    "key": "alt+cmd+0",
    "command": "-workbench.action.toggleEditorGroupLayout"
  },
  {
    "key": "ctrl+s ctrl+l",
    "command": "spotify.lyrics"
  },
  {
    "key": "ctrl+s ctrl+f",
    "command": "spotify.next"
  },
  {
    "key": "ctrl+cmd+]",
    "command": "-spotify.next"
  },
  {
    "key": "ctrl+s ctrl+b",
    "command": "spotify.previous"
  },
  {
    "key": "ctrl+cmd+[",
    "command": "-spotify.previous"
  },
  {
    "key": "ctrl+s ctrl+n",
    "command": "spotify.volumeDown"
  },
  {
    "key": "shift+cmd+;",
    "command": "-spotify.volumeDown"
  },
  {
    "key": "ctrl+s ctrl+p",
    "command": "spotify.volumeUp"
  },
  {
    "key": "shift+cmd+'",
    "command": "-spotify.volumeUp"
  },
  {
    "key": "ctrl+s t",
    "command": "vscode-spotify-tracks.focus"
  },
  {
    "key": "ctrl+s p",
    "command": "vscode-spotify-playlists.focus"
  },
  {
    "key": "ctrl+shift+cmd+p",
    "command": "workbench.view.extension.github-pull-requests"
  },
  {
    "key": "ctrl+cmd+tab",
    "command": "workbench.action.nextPanelView"
  },
  {
    "key": "ctrl+shift+cmd+tab",
    "command": "workbench.action.previousPanelView"
  },
  {
    "key": "shift+cmd+k",
    "command": "workbench.view.extension.kubernetesView"
  },
  {
    "key": "cmd+k cmd+n",
    "command": "extension.vsKubernetesUseNamespace"
  },
  {
    "key": "cmd+k cmd+t",
    "command": "extension.vsKubernetesTerminal"
  },
  {
    "key": "cmd+k cmd+l",
    "command": "extension.vsKubernetesShowLogs"
  }
]
