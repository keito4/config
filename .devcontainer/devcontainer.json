{
  "name": "Config Base Container",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "ghcr.io/devcontainers-extra/features/homebrew-package:1": {},
    "ghcr.io/eitsupi/devcontainer-features/jq-likes:2": {},
    "./features/common": {},
    "ghcr.io/itsmechlark/features/1password:1": {},
    "ghcr.io/devcontainers/features/github-cli:1": {},
    "ghcr.io/devcontainers/features/terraform:1": {},
    "ghcr.io/dhoeric/features/google-cloud-cli:1": {},
    "ghcr.io/devcontainers/features/aws-cli:1": {},
    "ghcr.io/devcontainers-community/npm-features/prettier:1": {},
    "ghcr.io/devcontainers-extra/features/supabase-cli:1": {},
    "ghcr.io/nils-geistmann/devcontainers-features/zsh:0": {},
    "ghcr.io/devcontainers-extra/features/zsh-plugins:0": {},
    "ghcr.io/devcontainers-extra/features/kubectl-asdf:2": {},
    "ghcr.io/dhoeric/features/act:1": {}
  },
  "remoteEnv": {
    "HOMEBREW_NO_AUTO_UPDATE": "1"
  },
  "overrideCommand": true,
  "updateRemoteUserUID": false,
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh",
        "terminal.integrated.profiles.linux": {
          "zsh": {
            "path": "/usr/bin/zsh",
            "args": [
              "-l"
            ]
          }
        }
      }
    }
  }
}
